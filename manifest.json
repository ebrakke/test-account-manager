{
    "manifest_version": 2,
    "name": "Test Account Manager",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/vendor/jquery.js", "js/content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "img/icon.png"
    },
    "background": {
        "scripts": ["js/background.js"]
    }
}
